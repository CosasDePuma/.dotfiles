" Plugins (vim-plug required)
call plug#begin('$HOME/.vim/plugged')
Plug 'junegunn/goyo.vim'
Plug 'neutaaaaan/iosvkem'
" Plug 'tyrannicaltoucan/vim-deep-space'
call plug#end()

" Themes
" colorscheme Iosvkem
set background=dark
set termguicolors

" Indentation and tabs
set expandtab
set autoindent
set tabstop=4
set shiftwidth=4
set softtabstop=4 
